{
  "name": "calendar-maker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@meforma/vue-toaster": "^1.3.0",
    "@types/moment": "^2.11.29",
    "@types/rrule": "^2.1.7",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^8.7.1",
    "ics": "^2.41.0",
    "moment": "^2.30.1",
    "rrule": "^2.8.1",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.3.4",
    "lint-staged": "^11.1.2",
    "typescript": "~4.5.5",
    "vite": "^2.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/standard",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "eqeqeq": 0,
      "space-before-function-paren": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
